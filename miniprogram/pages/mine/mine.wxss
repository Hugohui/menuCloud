/* pages/mine/mine.wxss */
page{
  background: #f6f6f6;
}
.mine-container{
  
}
.mine-container .do-list{

}
.mine-container .do-list button{
  width: 100%;
  height: 100%;
  line-height: 96rpx;
  border: none;
  background: none;
  border-radius: none;
  padding: 0;
  margin: 0;
  font-size: 36rpx;
  text-align:left;
}
.mine-container .do-list button::after{
  border: none;
  background: none;
  border-radius: none;
  padding: 0;
  margin: 0;
}
.mine-container .do-list .item{
  height: 96rpx;
  line-height: 96rpx;
  padding-left: 20rpx;
  font-size: 30rpx;
  text-align: left;
  border-bottom: 2rpx solid #ccc;
  position: relative;
}
.mine-container .do-list .item image{
  position: absolute;
  width: 45rpx;
  height: 45rpx;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}
